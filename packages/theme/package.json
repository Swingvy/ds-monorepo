        "@storybook/react": "^5.2.8",
        "@types/jest": "^24.0.25",
        "@types/react-test-renderer": "^16.9.1",
        "babel-loader": "^8.0.6",
        "jest": "^24.9.0",
        "jest-styled-components": "^6.3.4",
        "react-docgen": "^5.1.0",
        "react-test-renderer": "^16.12.0",
        "ts-jest": "^24.3.0"
